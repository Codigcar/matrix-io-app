{
  "name": "matrix",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "npx react-native run-android --mode=release",
    "android:aab": "cd android && ./gradlew bundleRelease && open ./app/build/outputs/bundle/release",
    "android:apk": "cd android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release",
    "android:apk:dev": "cd android && ./gradlew assembleDebug && open ./app/build/outputs/apk/debug",
    "android:apk:qa": "cd android && ./gradlew assembleQuality && open ./app/build/outputs/apk/quality",
    "android:dev": "npx react-native run-android --appIdSuffix=debug --mode=debug",
    "android:qa": "npx react-native run-android --appIdSuffix=quality --mode=quality",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format": "prettier --write",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "start:dev": "NODE_ENV=development npx react-native start --reset-cache",
    "start:prod": "NODE_ENV=prod npx react-native start --reset-cache",
    "start": "npx react-native start",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "build:ios:dev": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --platform='ios'",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "start:clean": "watchman watch-del-all && yarn start --reset-cache",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
